{
    "manifest_version": 3,
    "name": "ChatGPT Navigator",
    "version": "1.4",
    "description": "A simple extension to add navigation arrows to navigate between user prompts on ChatGPT website",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
      {
        "matches": [
          "https://chatgpt.com/*",
          "https://*.chatgpt.com/*",
          "https://chat.openai.com/*",
          "https://*.chat.openai.com/*"
        ],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  