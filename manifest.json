{
  "manifest_version": 3,
  "name": "ChatGPT Navigator",
  "version": "1.6",
  "description": "Use arrows to jump through ChatGPT messages quickly. Simple and smooth.",
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://*.chatgpt.com/*",
        "https://chat.openai.com/*",
        "https://*.chat.openai.com/*"
      ],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
